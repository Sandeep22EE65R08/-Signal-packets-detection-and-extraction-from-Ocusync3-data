# Compute and plot spectrogram for the original data
plt.figure(figsize=(100, 30))
plt.specgram(ocusync3_data, Fs=sampling_rate)
plt.title('Spectrogram of Original Data', fontsize=20, fontweight='bold')
plt.xlabel("Time (s)")
plt.ylabel("Frequency (Hz)")
plt.colorbar(label="Intensity (dB)")
plt.show()
